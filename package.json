{
  "name": "ember-metrics",
  "version": "0.13.0",
  "license": "MIT",
  "description": "Send data to multiple analytics integrations without re-implementing new API",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "start": "ember serve",
    "test": "COVERAGE=true ember test",
    "test:all": "ember try:each",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 88 --functions 75 --branches 64 --lines 88",
    "commit": "git-cz",
    "lint": "npm run lint:js && npm run lint:hbs",
    "posttest": "npm run report-coverage && npm run check-coverage && npm run validate-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poteto/ember-metrics.git"
  },
  "bugs": "https://github.com/poteto/ember-metrics/issues",
  "homepage": "https://github.com/poteto/ember-metrics",
  "engines": {
    "node": "10.* || >= 12"
  },
  "author": "Lauren Tan",
  "contributors": [
    {
      "name": "Michael Dupuis Jr"
    },
    {
      "name": "Mike North"
    }
  ],
  "dependencies": {
    "broccoli-funnel": "^3.0.2",
    "ember-cli-babel": "^7.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@ember/optional-features": "^1.3.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "ember-cli": "~3.16.2",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^4.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-sinon": "^4.0.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^1.13.2",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-bbva": "^3.2.2",
    "eslint-plugin-bbva": "^1.3.3",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-node": "^11.0.0",
    "husky": "^4.2.3",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "loader.js": "^4.7.0",
    "qunit-dom": "^1.0.0",
    "semantic-release": "^17.0.3"
  },
  "keywords": [
    "ember-addon",
    "metrics",
    "analytics",
    "segment",
    "tracking",
    "google analytics",
    "google tag manager",
    "mixpanel",
    "piwik",
    "intercom",
    "facebook-pixel"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
